# Gemini API Key
GEMINI_API_KEY=your_api_key_here

# Browser Settings
BROWSER_HEADLESS=False
BROWSER_WINDOW_WIDTH=1280
BROWSER_WINDOW_HEIGHT=720

# Agent Settings
MAX_WORKER_DEPTH=3
MASTER_TOKEN_LIMIT=2000
WORKER_TOKEN_LIMIT=8000
MAX_ACTION_ITERATIONS=10

# Timeouts (seconds)
TASK_TIMEOUT=120
ACTION_TIMEOUT=30
VERIFICATION_TIMEOUT=15

# Cache
LLM_CACHE_TTL=3600
DOM_CACHE_TTL=30
ENABLE_L1_CACHE=True
ENABLE_L2_CACHE=False

# Logging
LOG_LEVEL=INFO
ENABLE_DEBUG_OUTPUT=True

# OmniParser
OMNIPARSER_BOX_THRESHOLD=0.01
OMNIPARSER_IOU_THRESHOLD=0.9
USE_PADDLE_OCR=True
